if game.PlaceId == 13264238461 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | Anime Hatchers!", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

_G.rb = true
_G.egg = true


function rb()
     while _G.rb == true do
     game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("RebirthService"):WaitForChild("RF"):WaitForChild("rebirth"):InvokeServer()
     wait(0.1)
    end
end

function egg()
     while _G.egg == true do 
     local args = {
        [1] = 1
     }
     game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("hatchEgg"):InvokeServer(unpack(args))
     wait(0.0000000000000000000000001)
    end
end

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		_G.rb = Value
        rb()
	end    
})

MainTab:AddLabel("Note: Stay Near The Egg")

MainTab:AddToggle({
	Name = "Auto Hatch Cheapest Egg",
	Default = false,
	Callback = function(Value)
		_G.egg = Value
        egg()
	end    
})

end
